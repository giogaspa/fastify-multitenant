{
  "name": "@giogaspa/fastify-multitenant",
  "version": "1.0.0",
  "description": "Fastify plugin for multi-tenancy",
  "author": "Gioacchino Gasparella",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giogaspa/fastify-multitenant.git"
  },
  "keywords": [
    "fastify",
    "multi-tenant"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "node --loader ts-node/esm --test test/**/*.test.ts",
    "test:coverage": "node --loader ts-node/esm --test --experimental-test-coverage test/**/*.test.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "fastify": "^5.0.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^22.14.0",
    "fastify": "^5.2.2",
    "ts-node": "^10.9.2",
    "tsup": "8.3.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "fastify-plugin": "^5.0.1"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
